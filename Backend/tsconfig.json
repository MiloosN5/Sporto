{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["es2020", "DOM"],
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
  },
  "include": ["server.ts", "src/**/*", "types/**/*", "./@types/express"]
}

